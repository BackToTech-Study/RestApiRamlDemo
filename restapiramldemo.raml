#%RAML 1.0
title: RestApiRamlDemo

types:
  Account: !include DataTypes/Account.raml

  
/customers:
  get:
    queryParameters:
      requester:
        required: true
        type: string
        example: "Jhon"
      accountType:
        required: false
        type: !include DataTypes/AccountTypeValues.raml
      customerName:
        required: false
        type: string
    responses:
      200:
        body:
          application/json:
            type: Account[]